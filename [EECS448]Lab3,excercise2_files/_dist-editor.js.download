      this.buildPath = bind(this.buildPath, this);
      this.collectSkin = bind(this.collectSkin, this);
      this.collectColors = bind(this.collectColors, this);
      this.collectTabs = bind(this.collectTabs, this);
      this.fetchTabs = bind(this.fetchTabs, this);
      this.codeTypeToggle = bind(this.codeTypeToggle, this);
      this.initCreator = bind(this.initCreator, this);
      this.preferIframe = false;
      this.hasIframe = false;
      this.elements = {
        tabs: document.getElements("#creator .inputCont_checkbox input[type=checkbox][name=tabs]"),
        colors: document.getElements("#creator .inputCont_text input[type=text]"),
        skin: document.getElements("#creator .inputCont_checkbox input[type=radio][name=skin]"),
        code: document.getElements("#creator .embedCode"),
        preview: document.getElement("#preview"),
        codeType: document.getElement(".codeTypeToggle"),
        shareTrigger: document.getElement("#share")
      };
      this.events = {
        tabs: {
          change: this.collectTabs
        },
        colors: {
          keyup: this.collectColors
        },
        skin: {
          change: this.collectSkin
        },
        codeType: {
          click: this.codeTypeToggle
        },
        shareTrigger: {
          click: this.buildPath
        }
      };
      this.setupEvents(this.elements, this.events);
      this.builder = {
        tabs: [],
        colors: {},
        skin: "light"
      };
      this.fetchTabs();
    }

    EmbedCreator.prototype.initCreator = function() {
      var iframe;
      iframe = document.createElement("iframe");
      iframe.width = "100%";
      iframe.height = "300";
      iframe.frameBorder = "0";
      iframe.allowtransparency = true;
      iframe.sandbox = "allow-modals allow-forms allow-scripts allow-same-origin allow-popups";
      iframe.allow = "midi; geolocation; microphone; camera";
      iframe.allowfullscreen = true;
      iframe.allowpaymentrequest = true;
      this.elements.preview.adopt(iframe);
      return this.hasIframe = true;
    };

    EmbedCreator.prototype.codeTypeToggle = function(event) {
      event.stop();
      if (this.preferIframe) {
        this.elements.code[0].getParent().show();
        this.elements.code[1].getParent().hide();
        this.elements.codeType.set("text", "Prefer iframe?");
        return this.preferIframe = false;
      } else {
        this.elements.code[0].getParent().hide();
        this.elements.code[1].getParent().show();
        this.elements.codeType.set("text", "Use script");
        return this.preferIframe = true;
      }
    };

    EmbedCreator.prototype.fetchTabs = function() {
      var filtered;
      filtered = this.elements.tabs.filter(function(el) {
        if (el.checked) {
          return el;
        }
      });
      return this.builder.tabs = filtered.get("value");
    };

    EmbedCreator.prototype.collectTabs = function() {
      this.fetchTabs();
      return this.buildPath();
    };

    EmbedCreator.prototype.collectColors = function(event) {
      var element, key, value;
      element = event.target;
      value = element.get("value");
      key = element.get("name");
      if (value.length > 0) {
        if (value.indexOf('#') > -1) {
          value = value.split('#')[1];
        }
        this.builder.colors[key] = "" + value;
      } else {
        Object.erase(this.builder.colors, key);
      }
      return this.buildPath();
    };

    EmbedCreator.prototype.collectSkin = function() {
      var element;
      element = event.target;
