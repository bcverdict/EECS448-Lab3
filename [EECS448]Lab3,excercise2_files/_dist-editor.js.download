  initialize: function(options){
    this.options = options
    this.sections = getSelectors(this.options.sections)

    for (var i = 0; i < this.sections.length; i++){
      section = this.sections[i]
      toggler = section.querySelector(".toggler")

      toggler.addEventListener("click", function(event){
        this.showItem.call(this, event)
      }.bind(this))
    }

    // always open the first sidebar item
    this.sections[0].classList.add("opened")
