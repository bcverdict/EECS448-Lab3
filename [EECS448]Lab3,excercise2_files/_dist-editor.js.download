var Dropdown = new Class({

  initialize: function(){
    var self = this

    document.addEvents({
      "click:relay(.windowLabel, .dropdown a.dropdownTrigger)": function(event, element){
        if (event){
          event.preventDefault()
          event.stopPropagation()
        }

        self.showPopover.call(self, element)
      }
    })

    document.body.addEventListener("click", function(event){
      self.hideOutside.call(self, event)
    })
  },

  hideOutside: function(event){
    if (!event.target.getParent(".dropdownCont, .windowSettings")){
      this.hideAll()
    }
  },

  showPopover: function(trigger){
    var selector = "[data-popover-ref=" + trigger.dataset.popoverTrigger + "]"
    var popover  = document.querySelector(selector)
    var position = popover.dataset.popoverPosition

    this.hideAll()

    if (popover.classList.contains("hidden")){
      popover.classList.remove("hidden")
      var config = {
        relativeTo: trigger,
        position:   "bottomCenter",
        edge:       "topCenter",
        offset: {
          y: 6
        }
      }

      if (position === "left"){
        config.position = "bottomRight"
        config.edge     = "topRight"
      }

      popover.position(config)

      Track.ui("Show panel dropdown: " + trigger.dataset.panel)
    }
  },

  hideAll: function(){
    var allDropdownConts = document.querySelectorAll(".dropdownCont, [data-popover-ref]")
    allDropdownConts = Array.from(allDropdownConts)

    // hide all currently visible dropdowns
    allDropdownConts.forEach(function(dropdown){
      dropdown.classList.add("hidden")
    })
  },
})

