        }
        retStr = "";
        path = name.split(".");
        length = path.length;
        sub = object;
        if (length <= 1) {
          return retStr;
        }
        i = 0;
        while (i < length) {
          if ((sub = sub[path[i]]) === null) {
            return retStr;
          }
          i++;
        }
        return sub;
      });
    }
