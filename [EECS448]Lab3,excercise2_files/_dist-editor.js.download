  editorOptions: function(){
    var editorOptions = document.getElements("#editor-options input, #editor-options select");

    editorOptions.addEvents({
      change: function (event){
        var value = (this.get("type") === "checkbox" ? this.checked : this.get("value"));
        var key = this.get("name");

        var parseValue = function(value){
          if (parseInt(value) >= 0){
            return parseInt(value);
          } else {
            return value;
          }
        };

        value = parseValue(value);

        if (window.defaultCmOptions[key] === value){
          Object.erase(window.userCmOptions, key);
          if (key === "indentUnit"){
            Object.erase(window.userCmOptions, "tabSize");
          }
        } else {
          window.userCmOptions[key] = value;
          if (key === "indentUnit"){
            window.userCmOptions.tabSize = value;
          }
        }

        if (event){
          var styleEl = document.id("editor-theme")
          var url = ""
          if (key === "darkTheme"){
            if (value){
              url = styleEl.href.replace(/editor-.*?.css/, "editor-dark.css")
              styleEl.href = url
              Cookie.write("JSFIDDLE_theme", "dark", {
                duration: 200
              })
            } else {
              url = styleEl.href.replace(/editor-.*?.css/, "editor-light.css")
              styleEl.href = url
              Cookie.write("JSFIDDLE_theme", "light", {
                duration: 200
              })
            }
          }
        }

        Layout.editors.each(function(editorInstance){
          editorInstance.editor.setOption(key, value);
          if (key === "indentUnit"){
            editorInstance.editor.setOption("tabSize", value);
          }

          if (key === "codeLinting"){
            editorInstance.toggleLinting(editorInstance.editor.options.mode, value)
          }
        }, this);

        window.localStorage.setItem("JSFIDDLE_editor_options", JSON.encode(window.userCmOptions));

        Track.ui('Changed editor settings: ' + key)
      }
    });
  },

  switchLanguageAction: function(e) {
    if (!e) return;
    var sel = $(e.target);
    this.switchLanguage(sel);
  },

  /*
   * Change language in panel
   */
  switchLanguage: function(sel) {
    var panel_name  = sel.getParent(".windowSettings").get("data-panel"),
        editorClass = Layout.editors[panel_name],
        // Klass = MooShellEditor[panel_name.toUpperCase()],
        scriptLang  = sel.getParent(".windowSettings").getElement(".panelChoice option:selected"),
        language    = scriptLang.get("text"),
        mime        = scriptLang.get("data-mime-type"),
        extra       = sel.getParent(".windowSettings").getElement(".panelExtrasChoice select");

    if (!editorClass) return false;

    // CM can't decide if it's application/typescript or text/typescript
    if (mime === "text/typescript"){
      mime = "application/typescript"
    }

    // get all language info from passed mime type
    var langInfo = CodeMirror.findModeByMIME(mime)

    // switch language and lazy-load mode
    editorClass.editor.setOption("mode", langInfo.mime)
    CodeMirror.autoLoadMode(editorClass.editor, langInfo.mode)

    if (window.userCmOptions.codeLinting){
      forceMode = window.userCmOptions.codeLinting
    } else {
      forceMode = true
    }

    editorClass.toggleLinting(mime, forceMode)

    if (extra){
      languageExtra = language + " + " + extra.getElement("option:selected").get("text")
      Layout.editors[panel_name].setLabelName(languageExtra);
    } else {
      Layout.editors[panel_name].setLabelName(language);
    }

    window["panel_" + panel_name] = language.toLowerCase();
    this.showHideTidyUp();
    Track.ui("Switch language: " + language);
  },

  loadAsset: function(check, file, callback, scope) {
    if (!check()) {
      if (scope){
        callback = callback.bind(scope);
      }

      Asset.javascript(file, {
        onload: callback
      });
      return true;
    }
