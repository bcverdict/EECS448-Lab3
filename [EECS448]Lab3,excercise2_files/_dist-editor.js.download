}

window.cmOptions = Object.merge(window.defaultCmOptions, window.userCmOptions)

CodeMirror.modeURL = "/js/codemirror/mode/%N/%N.js"

var MooShellEditor = new Class({
  Implements: [Options, Events],

  parameter: 'Editor',

  options: {
    useCodeMirror: true,
    codeMirrorOptions: window.cmOptions,
    syntaxHighlighting: []
  },

  window_names: {
    'javascript':     'JavaScript',
    'html':           'HTML',
    'css':            'CSS',
    'scss':           'SCSS',
    'coffeescript':   'CoffeeScript',
    'javascript 1.7': 'JavaScript 1.7'
  },

  initialize: function(el, options) {
    this.validationTooltip;
    this.editorModified = false;

    if (!el) return false;

    this.element = $(el);
    if (!this.options.syntaxHighlighting.contains(options.language)) {
      this.forceDefaultCodeMirrorOptions();
    }
    this.setOptions(options);

    // var is_disallowed = (disallowedPlatforms.contains(Browser.Platform.name));
    // if (this.options.useCodeMirror && !is_disallowed) {
    if (this.options.useCodeMirror){

      // hide textarea
      this.element.hide();

      // prepare settings
      if (!this.options.codeMirrorOptions.stylesheet && this.options.stylesheet) {
        this.options.codeMirrorOptions.stylesheet = this.options.stylesheet.map(function(path) {
          return mediapath + path;
        });
      }
      if (!this.options.codeMirrorOptions.path) {
        this.options.codeMirrorOptions.path = codemirrorpath + 'js/';
      }
      if (!this.options.codeMirrorOptions.content) {
        this.options.codeMirrorOptions.content = this.element.get('value');
      }

      var parentNode = this.element.getParent();
      var options = {
        value: PanelValues[options.name]
      };
      options = Object.append(options, this.options.codeMirrorOptions);
      this.editor = CodeMirror(parentNode, options);

      emmetCodeMirror(this.editor);

      // run this after initialization
      if (!this.options.codeMirrorOptions.initCallback){
        if (this.options.codeMirrorOptions.autofocus) {
          // set current editor
          Layout.current_editor = this.options.name;
        }
      }

      this.setEditorEvents({
        // focus: function(){
        //   Layout.current_editor = this.options.name;
        // },
        change: this.contentChange.bind(this)
      });

      // disable new line insertion when saving fiddle
      CodeMirror.keyMap.default['Ctrl-Enter'] = function(){
        return false;
      };

      // don't let Emmet capture this
      delete CodeMirror.keyMap.default['Cmd-L'];
    }

    if (mooshell){
      mooshell.addEvents({
        'run': this.b64decode.bind(this)
      });
    }

    Layout.registerEditor(this);

    this.setLabelName(this.options.language || this.options.name);
    this.draftDiff(fiddleOptions.example_id);
  },

  contentChange: function(e){
    // this.validateEditorInput.call(this, parentNode);
    window.editorsModified = true;

    // WARNING: this is a weird workaround - change event is triggered on editor load
    // because of this draftStora was fired and the local draft object was
    // magically manipulated causing all sorts of werid behavior
    if (this.editorModified) {
      this.draftStore(fiddleOptions.example_id);
    }
    this.editorModified = true;

    // increase the counter so that periodical save knows if anything was updated
    Layout.counters.save += 1

    this.checkValidity()

    mooshell.autoRun()
  },

  checkValidity: function(){
    var linter = this.editor.state.lint

    if (linter){
      var issues = linter.marked.length
      mooshell.editorValidates = issues <= 0
    } else {
      mooshell.editorValidates = 0
    }
  },

  validateEditorInput: function(parentNode){
    var currentValue = this.getCode();
    var warnings = [];

    // destroy tooltip if it already exists for this editor
    if (this.validationTooltip){
      this.validationTooltip.destroy();
    }

    // create the container
    this.validationTooltip = Element('ul', {
      'class': 'warningTooltip'
    });

    // collect warnings
    if (this.options.disallowed){
      Object.each(this.options.disallowed, function(value, key){
        if (currentValue.test(key, 'i')){
          warnings.push('<li>' + value + '</li>');
        }
      });
    }

    // inject container
    this.validationTooltip = this.validationTooltip.inject(parentNode);

    // squash and apply warnings
    this.validationTooltip.set({
      html: warnings.join('')
    });
  },

  getEditor: function() {
    return this.editor || this.element;
  },

  getWindow: function() {
    if (!this.window) {
      this.window = this.element.getParent('.panel');
    }
    return this.window;
  },

  getLabel: function() {
    return this.getWindow().getElement('.windowLabel .label');
  },

  b64decode: function() {
    this.element.set('value', this.before_decode);
  },

  draftDiff: function(id){
    var draftTriggerCont = document.getElement(".draftTriggerCont");
    var localDraft = this.draftRead(id);

    if (localDraft && localDraft[this.options.name] && localDraft[this.options.name].code){
      draftTriggerCont.show();
      var base = difflib.stringAsLines(this.getCode());
      var draft = difflib.stringAsLines(localDraft[this.options.name].code);
      var sm = new difflib.SequenceMatcher(base, draft);
      var opcodes = sm.get_opcodes();

      var diff = diffview.buildView({
        baseTextLines: base,
        newTextLines: draft,
        opcodes: opcodes,
        viewType: 1
      });

      diffEl = document.id("diff-" + this.options.name);
      diffEl.adopt(diff);
    }
  },

  draftDifferent: function(id) {
    var localDraft = this.draftRead(id);
    return localDraft[this.options.name].code !== this.getCode();
  },

  draftApply: function(id) {
    var excludedSegments = ["gh", "api", "gist", "get", "post"];
    var path = document.location.pathname.split("/");

    // don't apply the draft in certain conditions
    if (excludedSegments.indexOf(path[1]) === -1 && path.length >= 2){
      var localDraft = this.draftRead(id);

      if (localDraft){
        if (localDraft[this.options.name]){
          if (this.draftDifferent(id)){
            this.setCode(localDraft[this.options.name].code);
          }
        } else {
          // console.warn("No draft for: ", this.options.name);
        }
      } else {
        // console.warn("No draft stored");
      }
    } else {
      // console.warn("Draft will not apply");
    }
  },

  draftStore: function(id) {
    Layout.editors.each(function(instance, key){
      Layout.localDraft[key] = {
        code: instance.getCode()
      }
      sessionStorage.setItem("draft[" + id + "]", this.draftEncode(Layout.localDraft));
    }, this);
  },

  draftRead: function(id) {
    var draft = sessionStorage.getItem("draft[" + id + "]")
    if (draft){
      return this.draftDecode(draft);
    }
  },

  draftRemove: function(id) {
    sessionStorage.removeItem("draft[" + id + "]")
  },

  draftEncode: function(code) {
    return Base64.encode(JSON.encode(code));
  },

  draftDecode: function(code) {
    return JSON.decode(Base64.decode(code));
  },

  setCode: function(value) {
    (this.editor) ? this.editor.setValue(value) : this.element.set('value', value);
  },

  getCode: function() {
    return (this.editor) ? this.editor.getValue() : this.element.get('value');
  },

  updateFromMirror: function() {
    this.before_decode = this.getCode();
    this.element.set('value', Base64.encode(this.before_decode));
  },

  updateCode: function() {
    this.element.set('value', this.getCode());
  },

  clean: function() {
    this.element.set('value', '');
    this.cleanEditor();
  },

  cleanEditor: function() {
    if (this.editor) this.editor.setCode('');
  },

  hide: function() {
    this.getWindow().hide();
  },

  show: function() {
    this.getWindow().show();
  },

  setEditorOptions: function(options){
    Object.each(options, function(fn, key){
      this.editor.setOption(key, fn.bind(this));
    }, this);
  },

  setEditorEvents: function(e){
    Object.each(e, function(fn, key){
      this.editor.on(key, fn.bind(this));
    }, this);
  },

  setLanguage: function(language) {
    // Todo: This is hacky
    this.setLabelName(language);
  },

  setLabelName: function(language) {
    this.getLabel().set('text', this.window_names[language] || language);
  },

  setStyle: function(key, value) {
    if (this.editor) return $(this.editor.frame).setStyle(key, value);
    return this.element.setStyle(key, value);
  },

  setStyles: function(options) {
    if (this.editor) return $(this.editor.frame).setStyles(options);
    return this.element.setStyles(options);
  },

  setWidth: function(width) {
    this.getWindow().setStyle('width', width);
  },

  setHeight: function(height) {
    this.getWindow().setStyle('height', height);
  },

  getPosition: function() {
    if (this.editor) return $(this.editor.frame).getPosition();
    return this.element.getPosition();
  },

  forceDefaultCodeMirrorOptions: function() {
    this.options.codeMirrorOptions = window.cmOptions;
  },

  toggleLinting: function(type, forceMode){

    // linter mode
    var linterMode = this.options.linters.contains(type)

    // linting options
    var options = {}

    // JS
    options["text/javascript"] = {
      esversion: 6,
      asi:       true
    }

    // Babel
    options["text/jsx"] = options["text/javascript"]

    // HTML5
    options["text/html"] = {
      rules: {
        "attr-value-double-quotes": false,
        "doctype-first":            false
      }
    }

    // CSS
    options["text/css"] = true

    // show or hide gutter for linter
    // toggle linting for specific langs (mime types)
    if (linterMode && forceMode){
      this.editor.setOption("gutters", ["CodeMirror-lint-markers"])
      this.editor.setOption("lint", options[type])
    } else {
      this.editor.setOption("gutters", [])
      this.editor.setOption("lint", false)
    }
  }
});


