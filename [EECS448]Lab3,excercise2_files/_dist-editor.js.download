var getGridSize = function(){
  var gridSizes = []

  for (var index = 0; index < currentInstance.length; index++){
    gridSizes.push(currentInstance[index].getSizes())
  }

  return gridSizes
}

var setGridSize = function(){
  var name = currentGrid
  var sizes = JSON.parse(localStorage.getItem(name + "-split-sizes"))

  if (sizes){
    for (var index = 0; index < currentInstance.length; index++){
      currentInstance[index].setSizes(sizes[index])
    }
  }
}

var saveGridSize = function(){
  var name = currentGrid
  localStorage.setItem(name + "-split-sizes", JSON.stringify(getGridSize()))
}

var options = function(direction){
  return {
    direction: direction || "horizontal",
    minSize: 100,
    snapOffset: 10,
    gutterSize: 1,
    onDragEnd: saveGridSize
  }
}

var switchGrid = function(mode, duringSwitch){
  localStorage.setItem("editor-grid-mode", mode)
  currentGrid = mode
  editor.innerHTML = grid[mode].structure
  currentInstance  = grid[mode].init()
  setGridSize()

  new MooShellEditor.JS(document.id("id_code_js"), {
    language: panel_js
  })

  new MooShellEditor.HTML(document.id("id_code_html"), {
    language: panel_html
  })
  new MooShellEditor.CSS(document.id("id_code_css"), {
    language: panel_css
  })

  // need to update JS pabel label becuse of the extra framework info
  mooshell.switchLanguage(document.querySelector("#panel_js_choice"))

  if (duringSwitch){
    mooshell.applyDraft()
  }
}

var initGrid = function(){
  var switchers = getElements("#layout input")
  var editor    = document.querySelector("#editor")

  for (var index = 0; index < switchers.length; index++){
    switchers[index].addEventListener("change", function(){
      switchGrid(this.value, true)
    })
  }

  if (localStorage.getItem("editor-grid-mode")){
    currentGrid = localStorage.getItem("editor-grid-mode")
    el = document.querySelector("input[name='editor_mode'][value='" + currentGrid + "']")
    el.checked = true
  }

  switchGrid(currentGrid, false)
};

