  autoRun: function(){
    var canAutoRun       = Layout.getEditorOptionValue("autoRun")
    var autoRunValidCode = Layout.getEditorOptionValue("autoRunValid")
    var enabledLinting   = Layout.getEditorOptionValue("codeLinting")

    if (canAutoRun){
      if (enabledLinting){

        // run only if code validates
        if (this.editorValidates && autoRunValidCode){
          this.runOnTimer()
        }
      } else {
        this.runOnTimer()
      }
    }
  },

  autoSave: function(){
    var saveFiddle = function(){
      var canAutoSave = Layout.getEditorOptionValue("autoSave")
      var saveCounter = Layout.counters.save

      // save only if option is enabled and something was actually added in the editors
      if (canAutoSave && saveCounter > 0){
        mooshell.save()

        // reset the counter so we don't save indefinitely
        Layout.counters.save = 0
      }
    }

    saveFiddle.periodical(60000)
